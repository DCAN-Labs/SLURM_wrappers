#!/bin/bash

subj_id=SUBJECTID
data_dir=DATADIR
run_dir=RUNDIR
#cpu_usage=8
singularity=`which singularity`

if [ ! -d ${data_dir}/derivatives ]; then
	mkdir -p ${data_dir}/derivatives
fi

env -i ${singularity} run \
-B ${data_dir}:/input \
-B ${data_dir}/derivatives/:/output \
/home/faird/shared/code/internal/pipelines/bibsnet_container/bibsnet_v3.0.2.sif \
/input /output participant -start prebibsnet -end postbibsnet -v

